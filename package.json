    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.17.0",
    "terraform": "^1.23.0",
    "tsx": "^4.16.5",
    "turbo": "^2.1.1"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=20"
  },
  "name": "turboviewer",
  "pnpm": {
    "patchedDependencies": {
      "graphql@16.9.0": "patches/graphql@16.9.0.patch"
