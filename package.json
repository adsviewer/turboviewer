{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "update": "pnpm update --latest && turbo update --concurrency=1 && pnpm install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prettier": "prettier --check \"**/*.{ts,tsx,md}\"",
    "lint-staged": "lint-staged --no-hide-partially-staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prisma": "^5.13.0",
    "terraform": "^1.23.0",
    "tsx": "^4.7.3",
    "turbo": "^1.13.3"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "patchedDependencies": {
      "graphql@16.8.1": "patches/graphql@16.8.1.patch"
    }
  }
}
